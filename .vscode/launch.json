    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI: Uvicorn",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "rag.app:app", // Replace 'main' with your Python file name without the .py extension, and 'app' with the FastAPI instance variable name.
                "--reload" // This enables auto-reload so the server restarts upon code changes.
            ],
            "jinja": false,
            "console": "integratedTerminal",
            "env": {
                // Set environment variables here. For example, you might want to specify the application's running environment or database URLs.
                // "ENVIRONMENT": "development",
                // "DATABASE_URL": "sqlite:///./test.db"
            }
        }
    ]
}